<Window x:Class="MailSender.Credentials"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MailSender"
        mc:Ignorable="d"
        Title="Credentials" Height="132.203" Width="377.542" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="label" Content="Username:" HorizontalAlignment="Left" Margin="5,10,0,0" VerticalAlignment="Top" Width="65" Height="23"/>
        <TextBox x:Name="tbUser" Height="23" Margin="73,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Left" Width="280"/>
        <Label x:Name="label_Copy" Content="Password:" HorizontalAlignment="Left" Margin="5,38,0,0" VerticalAlignment="Top" Width="65" Height="23"/>
        <Button x:Name="btnOk" Content="Ok" HorizontalAlignment="Left" Margin="278,66,0,0" VerticalAlignment="Top" Width="75" Click="btnOk_Click"/>
        <PasswordBox x:Name="pbPasswd" HorizontalAlignment="Left" Margin="73,38,0,0" VerticalAlignment="Top" Width="280" Height="23"/>

    </Grid>
</Window>
